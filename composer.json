{
    "name": "nas/konnec",
    "type": "project",
    "description": "Konnec NAS",
    "keywords": ["nas", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.5",
        "http-interop/http-factory-guzzle": "^1.2",
        "intervention/image": "^2.7",
        "konnec/vue-eloquent-api": "^0.3.0",
        "krizalys/onedrive-php-sdk": "^3.1",
        "laravel/framework": "^10.4",
        "laravel/helpers": "^1.6",
        "laravel/horizon": "^5.14",
        "laravel/octane": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^9.8",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.8",
        "meilisearch/meilisearch-php": "^1.0",
        "pbmedia/laravel-ffmpeg": "^8.1",
        "pusher/pusher-php-server": "^7.2",
        "sammyjo20/saloon": "^2.0",
        "s-ichikawa/laravel-sendgrid-driver": "^4.0",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft": "^4.1",
        "spatie/laravel-backup": "^8.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.5",
        "laravel/sail": "^1.20",
        "laravel/telescope": "^4.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.4",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Filesystem.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit 1024M"
        ],
        "larastan:baseline": [
            "./vendor/bin/phpstan analyse --generate-baseline --memory-limit 1024M"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --nowrite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "8.1",
            "ext-posix": "8.1"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    }
}
